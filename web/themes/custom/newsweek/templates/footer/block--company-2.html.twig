{% set bem_block = 'company' %}
<div class="{{ bem_block }}">
  <p class="{{ bem_block }}__title">Company</p>

  <div class="{{ bem_block }}__body">
    {% set items = {
      'About': '#',
      'Contact us': 'http://drupal-camp.localhost:8000/contact',
      'Subscription Plans' : '#',
      'My account' : 'http://drupal-camp.localhost:8000/user/login' }
    %}

    {% for item, link in items%}
      <a class="link" href="{{ link }}">{{ item }}</a>
    {% endfor %}
  </div>
</div>
